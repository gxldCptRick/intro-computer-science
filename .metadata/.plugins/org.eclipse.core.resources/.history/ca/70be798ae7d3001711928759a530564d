package test1;

import lib.ConsoleIO;

public class Program {
	
	
	public static void run() {

		String[] HangMan = {"(0)","/|\\", "/ \\"};
		
		buildHangMan();
		
	}
	
	private static void buildHangMan() {
		
		System.out.println("(0)");
		ConsoleIO.waitForInput();
		System.out.println("/");
		System.out.println("/");
		

	}
	
	

}
